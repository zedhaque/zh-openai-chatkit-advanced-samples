{
  "version": "1.0",
  "name": "flight_options_list",
  "template": "{%- set option_list = options | default([]) -%}\n{%- set selected_id = selectedId | default(None) -%}\n{%- set leg_value = leg | default(\"outbound\") -%}\n{\n  \"type\": \"ListView\",\n  \"children\": [\n    {%- for option in option_list %}\n    {%- set is_selected = selected_id == option.id %}\n    {\n      \"type\": \"ListViewItem\",\n      \"key\": {{ (option.id) | tojson }},\n      \"gap\": 0,\n      \"align\": \"stretch\",\n      \"onClickAction\": {% if is_selected %}{{ (none) | tojson }}{% else %}{\n        \"type\": {{ (actionType) | tojson }},\n        \"handler\": \"server\",\n        \"payload\": {\n          \"id\": {{ (option.id) | tojson }},\n          \"options\": {{ (option_list) | tojson }},\n          \"request\": {{ (request) | tojson }},\n          \"leg\": {{ (leg_value) | tojson }}\n        }\n      }{% endif %},\n      \"children\": [\n        {\n          \"type\": \"Box\",\n          \"width\": \"100%\",\n          \"padding\": 3,\n          \"border\": {\n            \"size\": 1,\n            \"color\": {% if is_selected %}\"blue-500\"{% else %}\"default\"{% endif %},\n            \"style\": {% if is_selected %}\"solid\"{% else %}\"dashed\"{% endif %}\n          },\n          \"radius\": \"xl\",\n          \"background\": \"surface\",\n          \"children\": [\n            {\n              \"type\": \"Row\",\n              \"gap\": 3,\n              \"align\": \"stretch\",\n              \"children\": [\n                {\n                  \"type\": \"Box\",\n                  \"width\": 3,\n                  \"background\": \"blue-600\",\n                  \"radius\": \"full\"\n                },\n                {\n                  \"type\": \"Col\",\n                  \"flex\": \"auto\",\n                  \"gap\": 2,\n                  \"children\": [\n                    {\n                      \"type\": \"Row\",\n                      \"children\": [\n                        {\n                          \"type\": \"Title\",\n                          \"value\": {{ (option.from_airport ~ \" \u2192 \" ~ option.to_airport) | tojson }},\n                          \"size\": \"md\"\n                        },\n                        {\"type\": \"Spacer\"},\n                        {\n                          \"type\": \"Badge\",\n                          \"label\": {{ (option.cabin) | tojson }},\n                          \"color\": \"discovery\",\n                          \"variant\": \"soft\"\n                        }\n                      ]\n                    },\n                    {\n                      \"type\": \"Row\",\n                      \"children\": [\n                        {\n                          \"type\": \"Col\",\n                          \"children\": [\n                            {\"type\": \"Caption\", \"value\": \"Depart\", \"color\": \"secondary\"},\n                            {\"type\": \"Title\", \"value\": {{ (option.dep_time) | tojson }}, \"size\": \"lg\"}\n                          ]\n                        },\n                        {\"type\": \"Spacer\"},\n                        {\n                          \"type\": \"Col\",\n                          \"align\": \"end\",\n                          \"children\": [\n                            {\"type\": \"Caption\", \"value\": \"Arrive\", \"color\": \"secondary\"},\n                            {\"type\": \"Title\", \"value\": {{ (option.arr_time) | tojson }}, \"size\": \"lg\"}\n                          ]\n                        }\n                      ]\n                    },\n                    {\"type\": \"Divider\"},\n                    {\n                      \"type\": \"Row\",\n                      \"children\": [\n                        {\"type\": \"Caption\", \"value\": {{ (option.date_label) | tojson }}},\n                        {\"type\": \"Spacer\"},\n                        {\"type\": \"Caption\", \"value\": {{ (option.from_airport ~ \" \u2022 \" ~ option.to_airport) | tojson }}}\n                      ]\n                    }\n                  ]\n                }\n              ]\n            }\n          ]\n        }\n      ]\n    }\n    {%- if not loop.last %},{%- endif %}\n    {%- endfor %}\n  ]\n}",
  "jsonSchema": {
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "options": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string",
              "minLength": 1
            },
            "from_airport": {
              "type": "string",
              "minLength": 1
            },
            "to_airport": {
              "type": "string",
              "minLength": 1
            },
            "dep_time": {
              "type": "string",
              "minLength": 1
            },
            "arr_time": {
              "type": "string",
              "minLength": 1
            },
            "date_label": {
              "type": "string",
              "minLength": 1
            },
            "cabin": {
              "type": "string",
              "minLength": 1
            }
          },
          "required": [
            "id",
            "from_airport",
            "to_airport",
            "dep_time",
            "arr_time",
            "date_label",
            "cabin"
          ],
          "additionalProperties": false
        }
      },
      "request": {
        "type": "object",
        "properties": {
          "origin": {
            "type": "string",
            "minLength": 1
          },
          "destination": {
            "type": "string",
            "minLength": 1
          },
          "depart_date": {
            "type": "string",
            "minLength": 1
          },
          "return_date": {
            "type": "string",
            "minLength": 1
          },
          "cabin": {
            "type": "string",
            "minLength": 1
          }
        },
        "required": [
          "origin",
          "destination",
          "depart_date",
          "return_date",
          "cabin"
        ],
        "additionalProperties": false
      },
      "selectedId": {

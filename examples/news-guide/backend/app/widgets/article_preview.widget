{
  "version": "1.0",
  "name": "Article preview",
  "template": "{\"type\":\"Basic\",\"children\":[{\"type\":\"Row\",\"gap\":3,\"padding\":0,\"align\":\"start\",\"children\":[{\"type\":\"Box\",\"border\":{\"color\":\"gray-900\",\"size\":1},\"width\":100,\"children\":[{\"type\":\"Image\",\"src\":{{ (heroImageUrl) | tojson }},\"alt\":{{ (title) | tojson }},\"fit\":\"cover\",\"position\":\"top\",\"width\":98,\"height\":98,\"frame\":true}]},{\"type\":\"Col\",\"gap\":1,\"children\":[{\"type\":\"Title\",\"value\":{{ (title) | tojson }},\"size\":\"sm\"},{\"type\":\"Row\",\"gap\":1,\"children\":[{\"type\":\"Text\",\"value\":{{ ((\"by \" ~ author)) | tojson }},\"size\":\"xs\"},{\"type\":\"Icon\",\"name\":\"dot\",\"size\":\"sm\"},{\"type\":\"Text\",\"value\":{{ (date) | tojson }},\"size\":\"xs\"}]}]}]}]}",

    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "minLength": 1
      },
      "title": {
        "type": "string",
        "minLength": 1
      },
      "author": {
        "type": "string",
        "minLength": 1
      },
      "heroImageUrl": {
        "type": "string",
        "format": "uri"
      },
      "date": {
        "type": "string",
        "minLength": 1
      }
    },
    "required": [
      "id",
      "title",
      "author",
      "heroImageUrl",
      "date"
    ],
    "additionalProperties": false
  },
  "outputJsonPreview": {

{
  "version": "1.0",
  "name": "article_list",
  "template": "{\"type\":\"ListView\",\"children\":[{%- set _c -%}{%-for article in articles -%},{\"type\":\"ListViewItem\",\"key\":{{ (article.id) | tojson }},\"children\":[{\"type\":\"Box\",\"maxWidth\":450,\"minWidth\":300,\"padding\":0,\"border\":{\"color\":\"gray-900\",\"size\":1},\"children\":[{\"type\":\"Row\",\"align\":\"stretch\",\"gap\":0,\"children\":[{\"type\":\"Image\",\"src\":{{ (article.heroImageUrl) | tojson }},\"alt\":{{ (article.title) | tojson }},\"fit\":\"cover\",\"position\":\"top\",\"height\":200,\"width\":160,\"radius\":\"none\",\"frame\":true},{\"type\":\"Col\",\"padding\":{\"x\":4,\"y\":3},\"gap\":2,\"flex\":1,\"align\":\"stretch\",\"justify\":\"between\",\"children\":[{\"type\":\"Col\",\"gap\":2,\"flex\":1,\"children\":[{\"type\":\"Text\",\"value\":{{ (article.date) | tojson }},\"color\":\"tertiary\",\"size\":\"xs\"},{\"type\":\"Text\",\"value\":{{ (article.title) | tojson }},\"size\":\"sm\",\"weight\":\"semibold\",\"maxLines\":4},{\"type\":\"Text\",\"value\":{{ ((\"by \" ~ article.author)) | tojson }},\"color\":\"tertiary\",\"size\":\"xs\"}]},{\"type\":\"Row\",\"justify\":\"end\",\"children\":[{\"type\":\"Button\",\"label\":\"View\",\"size\":\"sm\",\"iconSize\":\"sm\",\"pill\":true,\"variant\":\"ghost\",\"color\":\"warning\",\"iconEnd\":\"chevron-right\",\"onClickAction\":{\"type\":\"open_article\",\"handler\":\"client\",\"payload\":{\"id\":{{ (article.id) | tojson }}}}}]}]}]}]}]}{%-endfor-%}{%- endset -%}{{- (_c[1:] if _c and _c[0] == ',' else _c) -}}]}",
  "jsonSchema": {
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "articles": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string",
              "minLength": 1
            },
            "title": {
              "type": "string",
              "minLength": 1
            },
            "author": {
              "type": "string",
              "minLength": 1
            },
            "heroImageUrl": {
              "type": "string",
              "format": "uri"
            },
            "date": {
              "type": "string",
              "minLength": 1
            }
          },
          "required": [
            "id",
            "title",
            "author",
            "heroImageUrl",
            "date"
          ],
          "additionalProperties": false
        }
      }
    },
    "required": [
      "articles"
    ],
    "additionalProperties": false
  },
